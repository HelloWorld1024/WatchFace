<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    <EditTextPreference android:title="语速" android:key="speed_preference" android:summary="默认值：50 " android:defaultValue="50" android:dialogTitle="请输入语速:在线(0-100)本地(0-200)" />
    <EditTextPreference android:title="音调" android:key="pitch_preference" android:summary="默认值：50 " android:defaultValue="50" android:dialogTitle="请输入音调(0-100)" />
    <EditTextPreference android:title="音量" android:key="volume_preference" android:summary="默认值：50 " android:defaultValue="50" android:dialogTitle="请输入音量(0-100)" />
    <ListPreference android:entries="@array/stream_entries" android:title="音频流类型" android:key="stream_preference" android:defaultValue="3" android:entryValues="@array/stream_values" />
</PreferenceScreen>